<h5 class="tall"><b>Vendor</b></h5>
{{ form_row(form.fullName) }}
{{ form_row(form.email) }}
{{ form_row(form.plainPassword) }}
<h5 class="tall"><b>Address</b></h5>
{{ form_row(form.street) }}
{{ form_row(form.city) }}
{{ form_row(form.state) }}
<div class="styled-select">
    {{ form_row(form.country) }}
</div>
{{ form_row(form.zipCode) }}
<h5 class="tall"><b>Vendor Contact Person</b></h5>
{{ form_rest(form) }}
<script>
    $('input,select').trigger('change');
</script>