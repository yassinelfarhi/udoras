{{ form_stylesheet(form) }}
{{ form_start(form, {'attr': {'class':'form-horizontal'}}) }}
{% if type == "FS\\UserBundle\\Entity\\Employee" %}
    {% include "@FSUser/FormFormat/employee.html.twig" %}
{% elseif type == "FS\\UserBundle\\Entity\\Vendor" %}
    {% include "@FSUser/FormFormat/vendor.html.twig" %}
{% endif %}

<div class="form-group form-text">
    <span>
        By clicking on "Create Account" button, you agree with our
        <a href="{{ path('fs_basic_privacy_policy_page') }}">
            Privacy Policy
        </a>
    </span>
</div>
<div class="form-group form-text">
    <p style="margin-top: 10px">
        <a href="{{ path('fs_basic_homepage') }}" class="btn btn-shadow btn-lg btn-back">Back</a>
        <button type="submit" class="btn btn-shadow btn-lg pull-right ajax-submit">Create Account
        </button>
    </p>
</div>
{{ form_end(form) }}
<script>
    $(function(){
        $('input').trigger('change'); // reflow labels
    });
</script>